      ["DELETE_CARD", "$VILLAIN_CARD.id"],
      ["DEFINE", "$VILLAIN_CARD", ["ONE_CARD", "$CARD", ["EQUAL", "$CARD.databaseId", "3ec35abb-ad36-5a9a-88b7-33436c3478fc"]]],
      ["DELETE_CARD", "$VILLAIN_CARD.id"],
      ["DEFINE", "$VILLAIN_CARD", ["ONE_CARD", "$CARD", ["EQUAL", "$CARD.databaseId", "922092fa-fcdd-50ee-8a4f-37bc68843ee6"]]],
      ["DELETE_CARD", "$VILLAIN_CARD.id"],
      ["DEFINE", "$VILLAIN_CARD", ["ONE_CARD", "$CARD", ["EQUAL", "$CARD.databaseId", "20d73026-d831-5b50-8d3b-2d2be4bdf0ab"]]],
      ["DELETE_CARD", "$VILLAIN_CARD.id"],
      ["DEFINE", "$VILLAIN_CARD", ["ONE_CARD", "$CARD", ["EQUAL", "$CARD.databaseId", "8da7c346-63f0-590c-8960-92bd0337787f"]]],
